
.watches
  #add-watch
    .err-msg 
      display inline-block
      margin 0
      padding 6px 12px
  tr
    &:hover 
      cursor pointer
  .delete-watch:hover
      color red
  

.activity
  .axis path,
  .axis line 
    fill none
    stroke #000
    shape-rendering crispEdges

  .line 
    fill none
    stroke steelblue
    stroke-width 1.5px
